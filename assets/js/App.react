import React, { Component } from 'react';
import ReactDOM from 'react-dom';
import moment from 'moment';
import Day from './Day';
import Store from './Store';

class App extends Component {
  constructor(props) {
    super(props);
    this.state = {
      store: new Store()
    };
  }

  render() {
    return (
      <div>
        <Day date={moment()} store={this.state.store}/>
      </div>
    );
  }
}

export default App;
